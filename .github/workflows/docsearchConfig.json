{
  "index_name": "dev-ethereum-org",
  "start_urls": ["https://deploy-preview-1134--ethereumorg.netlify.app/en/"],
  "stop_urls": [],
  "selectors": {
    "lvl0": "main h1",
    "lvl1": "main h2",
    "lvl2": "main h3",
    "lvl3": "main h4",
    "lvl4": "main h5",
    "text": "main p, article li",
    "lang": {
      "selector": "/html/@lang",
      "type": "xpath",
      "global": true,
      "default_value": "en"
    }
  },
  "selectors_exclude": ["aside"],
  "strip_chars": " .,;:#",
  "custom_settings": {
    "attributesForFaceting": ["lang"]
  },
  "scrape_start_urls": false
}
